{
    "version": "0.2",
    "ignorePaths": [
        "eng/common",
        "eng/release/DiagnosticsReleaseTool",
        "src/external",
        "src/inc",
        "*.Designer.cs"
    ],
    "overrides": [
        {
            "filename": "*.resx",
            "languageId": "xml"
        },
        {
            "filename": "*{.targets,.props,.*proj}",
            "languageId": "msbuild"
        }
    ],
    "words": [
        "arity",
        "armv",
        "Bldr",
        "blittable",
        "Brotli",
        "Browsable",
        "callstacks",
        "cctor",
        "CLSID",
        "codespace",
        "codespaces",
        "configurator",
        "coreclr",
        "darc",
        "dbug",
        "Demultiplexer",
        "disambiguator",
        "discoverability",
        "Distro",
        "distroless",
        "dockerfiles",
        "dylib",
        "entra",
        "ESRP",
        "evented",
        "exfiltrate",
        "fwlink",
        "gcdump",
        "gcdumps",
        "globbing",
        "globstar",
        "HCORENUM",
        "hmac",
        "HMACSHA",
        "hresult",
        "Hsts",
        "Impls",
        "JITID",
        "JWTs",
        "LCID",
        "Ldstr",
        "libc",
        "livemetrics",
        "minidump",
        "minio",
        "mitigations",
        "msbuild",
        "msdata",
        "MSRC",
        "mvid",
        "ndjson",
        "netcoreapp",
        "newtonsoft",
        "Notfication",
        "NTLM",
        "nupkg",
        "openapi",
        "overreporting",
        "PCCOR",
        "Pids",
        "Pinnable",
        "PINVOKE",
        "Pkce",
        "PKCS",
        "pkgs",
        "Posix",
        "pwsz",
        "quantile",
        "rclsid",
        "reentrancy",
        "REFCLSID",
        "REFIID",
        "rejit",
        "Remoting",
        "Renormalize",
        "respecifying",
        "resx",
        "rethrown",
        "riid",
        "RSASSA",
        "runtimes",
        "speedscope",
        "Tfms",
        "threadpool",
        "trce",
        "tstr",
        "ukwn",
        "uninitialize",
        "Uninstallation",
        "uniquifier",
        "Unlocalized",
        "Unredacted",
        "Unthrown",
        "upvoting",
        "VSAPPIDDIR",
        "walkthroughs",
        "WSADATA",
        "Xunit"
    ],
    "patterns": [
        {
            "name": "markdown_code_block",
            "pattern": "/^(\\s*`{3,}).*[^```]*?^\\1/gmx"
        },
        {
            "name": "markdown_inline_code",
            "pattern": "/`[^`\\n\\r]*`/gmx"
        },
        {
            "name": "preprocessor_conditional",
            "pattern": "/^[^\\S\\n\\r]*\\#(el)?if (.)*$/gm"
        },
        {
            "name": "test_data",
            "pattern": "/^[^\\S\\n\\r]*\\[(Inline|Member)Data\\(.*$/gm"
        },
        {
            "name": "using_statement",
            "pattern": "/^[^\\S\\n\\r]*using .*;$/gm"
        },
        {
            "name": "error_codes",
            "pattern": "/[A-Z_]+(E|S)_\\w+/gm"
        },
        {
            "name": "il_opcodes",
            "pattern": "/CEE_[A-Z_]+/gm"
        },
        {
            "name": "xml_comment_block",
            "pattern": "/<!--[\\s\\S]*?-->/gm"
        }
    ],
    "languageSettings": [
        {
            "languageId": [
                "markdown"
            ],
            "ignoreRegExpList": [
                "markdown_code_block",
                "markdown_inline_code"
            ]
        },
        {
            "languageId": [
                "csharp"
            ],
            "allowCompoundWords": true,
            "ignoreRegExpList": [
                "preprocessor_conditional",
                "test_data",
                "using_statement"
            ]
        },
        {
            "languageId": [
                "cpp"
            ],
            "allowCompoundWords": true,
            "ignoreRegExpList": [
                "preprocessor_conditional",
                "il_opcodes",
                "error_codes"
            ]
        },
        {
            "languageId": [
                "xml"
            ],
            "allowCompoundWords": true,
            "ignoreRegExpList": [
                "xml_comment_block"
            ]
        },
        {
            "languageId": [
                "msbuild"
            ],
            "allowCompoundWords": true
        }
    ]
}
