<Project>
  <Import Project="$(MSBuildThisFileDirectory)..\Directory.Build.props" />
  <PropertyGroup>
    <!-- By default, do not ship archives. Symbols NuGet packages should never ship. -->
    <IsShipping>false</IsShipping>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)Archive.props" Condition="'$(IsArchivable)' == 'true'" />
  <Import Project="$(MSBuildThisFileDirectory)Pack.props" Condition="'$(IsPackable)' == 'true'" />
</Project>
