{
  "LargeGCHeap": {
    "Actions": [
      {
        "Settings": {
          "Egress": "artifacts" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
        },
        "Type": "CollectGCDump" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
      }
    ],
    "Filters": [
      {
        "Key": "ProcessName" /*JsonConfigurationProvider for 'settings.json' (Optional)*/,
        "Value": "FirstWebApp" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
      }
    ],
    "Limits": {
      "ActionCount": "2" /*JsonConfigurationProvider for 'settings.json' (Optional)*/,
      "ActionCountSlidingWindowDuration": "1:00:00" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
    },
    "Trigger": {
      "Settings": {
        "CounterName": "gc-heap-size" /*JsonConfigurationProvider for 'settings.json' (Optional)*/,
        "GreaterThan": "10" /*JsonConfigurationProvider for 'settings.json' (Optional)*/,
        "ProviderName": "System.Runtime" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
      },
      "Type": "EventCounter" /*JsonConfigurationProvider for 'settings.json' (Optional)*/
    }
  }
}
