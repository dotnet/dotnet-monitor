{
  "Rule1": {
    "Actions": [
      {
        "Type": "CollectGCDump",
        "Settings": {
          "Egress": "artifacts"
        }
      },
      {
        "Settings": {
          "Egress": "monitorBlob"
        },
        "Type": "CollectDump"
      }
    ],
    "Filters": [
      {
        "Key": "ProcessName",
        "Value": "FirstWebApp"
      }
    ],
    "Limits": {
      "ActionCount": "3",
      "ActionCountSlidingWindowDuration": "1:00:00",
      "RuleDuration": "00:00:05"
    },
    "Trigger": {
      "Type": "AspNetRequestCount",
      "Settings": {
        "RequestCount": "10",
        "SlidingWindowDuration": "00:05:00"
      }
    }
  },
  "Rule2": {
    "Actions": [
      {
        "Settings": {
          "Type": "Mini",
          "Egress": "artifacts"
        },
        "Type": "CollectDump"
      }
    ],
    "Filters": [
      {
        "Key": "ProcessName",
        "Value": "FirstWebApp"
      }
    ],
    "Trigger": {
      "Settings": {
        "StatusCodes": [ "400-409" ],
        "ResponseCount": "1",
        "SlidingWindowDuration": "00:05:00"
      },
      "Type": "AspNetResponseStatus"
    },
    "Limits": {
      "ActionCount": "20",
      "ActionCountSlidingWindowDuration": "00:00:01",
      "RuleDuration": "11:11:11"
    }
  },
  "Rule3": {
    "Actions": [
      {
        "Settings": {
          "Duration": "00:01:00",
          "Profile": "Cpu",
          "Egress": "artifacts"
        },
        "Type": "CollectTrace"
      }
    ],
    "Limits": {
      "ActionCountSlidingWindowDuration": "5:00:00",
      "ActionCount": "20",
      "RuleDuration": "11:11:11"
    },
    "Trigger": {
      "Settings": {
        "RequestCount": "123",
        "SlidingWindowDuration": "00:00:02"
      },
      "Type": "AspNetRequestDuration"
    }
  }
}
