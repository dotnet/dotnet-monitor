{
  "LargeGCHeap": {
    "Actions": [
      {
        "Settings": {
          "Egress": "artifacts"
        },
        "Type": "CollectGCDump"
      }
    ],
    "Filters": [
      {
        "Key": "ProcessName",
        "Value": "FirstWebApp"
      }
    ],
    "Limits": {
      "ActionCount": "2",
      "ActionCountSlidingWindowDuration": "1:00:00"
    },
    "Trigger": {
      "Settings": {
        "CounterName": "gc-heap-size",
        "GreaterThan": "10",
        "ProviderName": "System.Runtime"
      },
      "Type": "EventCounter"
    }
  }
}
