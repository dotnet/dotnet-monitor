{
  "CollectionRules": {
    "Rule1": {
      "Trigger": {
        "Type": "AspNetRequestCount"
      },
      "Actions": [
        {
          "Type": "CollectGCDump"
        },
        {
          "Type": "CollectDump",
          "Settings": {
            "Egress": "monitorBlob"
          }
        }
      ],
      "Filters": [
        {
          "Key": "ProcessName",
          "Value": "FirstWebApp"
        }
      ],
      "Limits": {
        "ActionCountSlidingWindowDuration": "1:00:00",
        "ActionCount": 3,
        "RuleDuration": "00:00:05"
      }
    },
    "Rule2": {
      "Trigger": {
        "Type": "AspNetResponseStatus",
        "Settings": {
          "StatusCodes": ["400-409"]
        }
      },
      "Actions": [
        {
          "Type": "CollectDump",
          "Settings": {
            "Type": "Mini"
          }
        }
      ],
      "Filters": [
        {
          "Key": "ProcessName",
          "Value": "FirstWebApp"
        }
      ]
    }
  }
}
