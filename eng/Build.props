    <ProjectToBuild Include="$(RepoRoot)src\archives\symbols\dotnet-monitor\dotnet-monitor-symbols.proj">
      <AdditionalProperties>TargetFramework=net7.0;RuntimeIdentifier=$(PackageRid)</AdditionalProperties>
    </ProjectToBuild>
  </ItemGroup>
</Project>
